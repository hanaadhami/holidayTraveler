{{#each holidays}}
<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">{{ this.name }}</h5>
    <h6 class="card-subtitle mb-2 text-muted">Year: {{ this.date.datetime.year }} Month: {{ this.date.datetime.month }} Date: {{ this.date.datetime.day }}</h6>
    <p class="card-text">Description: {{ this.description }}</p>
  </div>
</div>
{{/each}}

