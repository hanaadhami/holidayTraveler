<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
  integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">

<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css"
  integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous" />

<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

<style>
  body {
  background-image: url("../../images/Beautiful_Sunrise_Background-448.jpg")
  }
</style>

<div class="container">
  <ul class="collapsible">
    <li class="active">
      <div class="collapsible-header"><i class="material-icons">language</i>Holidays</div>
      <div class="collapsible-body" style="display: block;"><span>
        {{#each holidays}}
          <div class='s12 scrollsSpy'>
          <div class="card col-md-3" style="width: 25rem; height: 25rem;">
            <div class="card-body " style="display: flex; flex-direction: column;">
              <h5 class="card-title">{{ this.name }}</h5>
              <hr>
              <h6 class="card-subtitle mb-2 text-muted">Year: {{ this.date.datetime.year }} Month:
                {{ this.date.datetime.month }} Date: {{ this.date.datetime.day }}</h6>
              <p class="card-text">Description: {{ this.description }}</p>
              <a href="/members">Back to search</a>
            </div>
          </div>
          </div>
          {{/each}}
        </span></div>
    </li>
    <li>
      <div class="collapsible-header"><i class="material-icons">turned_in</i>Second</div>
      <div class="collapsible-body"><span>Lorem ipsum dolor sit amet.</span></div>
    </li>
  </ul>





  <script src="../../js/countryList.js"></script>